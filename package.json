{
  "name": "ai-lms-system",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:critical": "jest --testPathPattern='(CourseContext|streamingService|CourseEditor)'",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint && npm run test:ci",
    "prepare": "husky",
    "remotion:preview": "remotion preview src/remotion/index.ts",
    "remotion:studio": "remotion studio src/remotion/index.ts",
    "remotion:render": "remotion render src/remotion/index.ts"
  },
  "dependencies": {
    "@dataconnect/generated": "file:src/dataconnect-generated",
    "@google/generative-ai": "^0.24.1",
    "@remotion/cli": "^4.0.409",
    "@remotion/player": "^4.0.409",
    "@tabler/icons-react": "^3.36.0",
    "@tiptap/extension-link": "^3.14.0",
    "@tiptap/extension-placeholder": "^3.14.0",
    "@tiptap/extension-text-align": "^3.14.0",
    "@tiptap/extension-underline": "^3.14.0",
    "@tiptap/react": "^3.14.0",
    "@tiptap/starter-kit": "^3.14.0",
    "dompurify": "^3.3.1",
    "dotenv": "^17.2.3",
    "firebase": "^12.6.0",
    "firebase-tools": "^15.1.0",
    "googleapis": "^169.0.0",
    "html2canvas": "^1.4.1",
    "html2pdf.js": "^0.12.1",
    "idb": "^8.0.3",
    "jspdf": "^4.0.0",
    "katex": "^0.16.27",
    "openai": "^6.15.0",
    "pdfjs-dist": "^4.10.38",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-katex": "^3.1.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.22.3",
    "reactflow": "^11.11.4",
    "recharts": "^3.6.0",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "remotion": "^4.0.409",
    "uuid": "^13.0.0",
    "xlsx": "^0.18.5",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@remotion/bundler": "^4.0.409",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/dompurify": "^3.0.5",
    "@types/html2pdf.js": "^0.10.0",
    "@types/jest": "^29.5.14",
    "@types/katex": "^0.16.7",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "firebase-admin": "^13.6.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "ts-jest": "^29.4.6",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vitest": "^4.0.16"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npx jest --bail --findRelatedTests --passWithNoTests"
    ]
  }
}
